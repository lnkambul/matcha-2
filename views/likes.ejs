<%- include ('partials/header') %>
<h3 class="text-warning">history of likes</h3>
<div class="row row-cols-1 row-cols-md-4">
	<% for (let i in likes) {%>
		<div class="col mb-3">
			<div class="card text-white bg-transparent">
				<img src="/uploads/<%=likes[i].username%>" onerror="this.onerror=null;this.src='/uploads/avatar.jpg';" class="card-img" height="220">
				<a href="/p/<%=likes[i].username%>" style="text-decoration:none">
					<div class="card-body card-img-overlay text-danger">
						<h2 class="text-warning"><%=likes[i].username%></h2>
					</div>
				</a>
			</div>
		</div>
	<%}%>
</div>
<%- include ('partials/footer') %>
